{
  "name": "billing-interface",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "db:seed": "node scripts/seed/",
    "db:push:prod": "pnpm drizzle-kit push",
    "db:pull:prod": "pnpm drizzle-kit pull",
    "db:gene:prod": "pnpm drizzle-kit generate",
    "db:migr:prod": "pnpm drizzle-kit migrate",
    "db:push": "pnpm drizzle-kit push --config=drizzle-dev.config.ts",
    "db:pull": "pnpm drizzle-kit pull --config=drizzle-dev.config.ts",
    "db:gene": "pnpm drizzle-kit generate --config=drizzle-dev.config.ts",
    "db:migr": "pnpm drizzle-kit migrate --config=drizzle-dev.config.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^6.1.1",
    "@sveltejs/kit": "^2.50.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^24.10.9",
    "@types/pg": "^8.16.0",
    "drizzle-kit": "^0.31.8",
    "svelte": "^5.49.0",
    "svelte-check": "^4.3.5",
    "typescript": "^5.9.3",
    "vite": "^7.3.1"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "dependencies": {
    "@lucide/svelte": "^0.563.1",
    "bcryptjs": "^3.0.3",
    "dayjs": "^1.11.19",
    "drizzle-orm": "^0.44.7",
    "extend-arabic-query": "^0.3.2",
    "jose": "^6.1.3",
    "lodash.debounce": "^4.0.8",
    "pg": "^8.17.2",
    "svelte-sonner": "^1.0.7"
  }
}
